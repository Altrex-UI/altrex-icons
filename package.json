{
  "name": "@altrex-ui/icons",
  "version": "0.1.3",
  "description": "Altrex Icons - SVG icon library",
  "type": "module",
  "exports": {
    ".": "./altrex.svg",
    "./altrex.svg": "./altrex.svg",
    "./icons.json": "./icons.json",
    "./iconify/altrex-free.json": "./iconify/altrex-free.json",
    "./iconify/altrex-brand.json": "./iconify/altrex-brand.json",
    "./iconify/altrex-flag.json": "./iconify/altrex-flag.json"
  },
  "scripts": {
    "clean": "rimraf altrex.svg icons.json iconify",
    "optimize": "svgo -r -f ./icons/",
    "sprite": "node ./scripts/createIconsJson.cjs",
    "json": "node ./scripts/createSvgSprite.js",
    "iconify": "node ./scripts/exportIconify.js",
    "build": "npm run clean && npm run optimize && npm run sprite && npm run json && npm run iconify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Altrex-UI/altrex-icons.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Icons",
    "Svg",
    "Vector",
    "Sprite",
    "Iconify"
  ],
  "author": {
    "name": "Altrex UI",
    "url": "https://github.com/Altrex-UI"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Altrex-UI/altrex-icons/issues"
  },
  "homepage": "https://github.com/Altrex-UI/altrex-icons#readme",
  "devDependencies": {
    "@iconify/tools": "^3.0.7",
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^2.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.13",
    "clean-css-cli": "^5.6.2",
    "concat": "^1.0.3",
    "eslint": "^8.35.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "rimraf": "^4.3.1",
    "rollup": "^3.18.0",
    "sass": "^1.58.3",
    "stylelint": "^16.8.0",
    "stylelint-config-twbs-bootstrap": "^16.1.0",
    "svg-sprite": "^2.0.4",
    "svgo": "^3.3.2",
    "typescript": "^4.9.5"
  },
  "files": [
    "altrex.svg",
    "icons.json",
    "iconify/",
    "icons/"
  ]
}
